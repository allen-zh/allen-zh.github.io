RewriteEngine on 
RewriteBase / 

## 轉向所有domain.com 去www.domain.com
RewriteCond %{HTTP_HOST} ^standmac\.com$ [NC]
RewriteRule ^(.*)$ http://www.standmac.com/$1 [L,R=301]

## 所有的沒有結尾斜線但是又是指向資料夾的URL，轉向成有結尾斜線的URL
RewriteRule ^new$ new/ [L,R=302]

RewriteCond %{HTTP_USER_AGENT} android|blackberry|ip(hone|od|ad)|kindle|symbian|wap [NC]
RewriteRule ^new/(.*)$ v1/page/m/$1
RewriteRule ^new/(.*)$ v1/page/pc/$1

